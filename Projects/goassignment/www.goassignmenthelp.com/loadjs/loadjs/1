$("#fileupload").submit(function (e) {
    e.preventDefault();
    var attachment = "No";
    var nicE = new nicEditors.findEditor('main_form_details');
    var description = jQuery('<div />').text(nicE.getContent()).html();
    if (ajax) {
        showalert("Already request in process");
        return
    }
    if (!$("#fileupload").valid()) {
        showalert("Please fill all the required fields");
        grecaptcha.reset(widget2);
        return
    }
    if ($(this).find(".orderfile").val() == "") {
        if (description.length < 100) {
            // Use jQuery Validate to show the error for the 'details' field
            var validator = $("#fileupload").validate();
            validator.showErrors({
                "details": "Description should be at least 100 characters or select a file"
            });
            grecaptcha.reset(widget2);
            return;
        }
    } else {
        description = (nicE.getContent() == "Place Your Question Here") ? "PFA" : description;
        attachment = "Yes";
    }

    if ($("#firstCaptcha").val() == "") {
        showalert("Captcha is required");
        return;
    }
    try {
        var username = $(this).find('input[name="email"]').val();
        var assignment = {
            topicId: $(this).find('input[name="topicId"]').val(),
            subTopicId: $(this).find('input[name="subTopicId"]').val(),
            gradeId: 5,
            timeZone: /\((.*)\)/.exec(new Date().toString())[1],
            domainId: 8,
            title: description,
            description: description,
            deadline: new Date($(this).find('input[name="date"]').val()).toUTCString().toString(),
            ipAddress: ip,
            submitUrl: "goah.com",
            nameYourPrice: 10,
            questionSubmitSourceType: 62,
            SubmitSourceId: $(this).find('input[name="ss_id"]').val(),
            "user": {
                name: "",
                emailId: $(this).find('input[name="email"]').val(),
                state: state,
                country: country,
                countryCode: $(this).find('input[name="country_code"]').val(),
                phone: $(this).find('input[name="phone"]').val()
            }
        };
        var internal_lead = {
            email: $(this).find('input[name="email"]').val(),
            topicid: $(this).find('input[name="topicId"]').val(),
            subtopicid: $(this).find('input[name="subTopicId"]').val(),
            description: description,
            deadline: new Date($(this).find('input[name="date"]').val()).toUTCString().toString(),
            ip: ip,

            attachment: attachment,

        };
        var post_assign = JSON.stringify(assignment);
    } catch (err) {
        showalert(err.message);
    }

    ajax = 1;
    showOverlay(1);
    var data = new FormData();
    $.each($(this).find('input[name="uploadfile"]')[0].files, function (i, file) {
        data.append('imageFile' + i, file);
    });
    data.append("assignment", post_assign);
    var progressBar = document.getElementById("progress");
    $.ajax({
        url: baseProtocol + "://" + api_url + "/api/AssignmentSubmit/CreateAssignment",
        type: "post",
        data: data,
        contentType: false,
        cache: false,
        processData: false,
        dataType: "json",
        beforeSend: function () {
            progressBar.value = 0;
            /* $("#progress-display").html('0%');*/
        },
        xhr: function () {

            var xhr = $.ajaxSettings.xhr();
            //Upload progress
            xhr.upload.onprogress = function (evt) {
                if (evt.lengthComputable) {
                    progressBar.max = evt.total;
                    progressBar.value = evt.loaded;
                    var percentComplete = Math.floor((evt.loaded / evt.total) * 100);
                    //Do something with upload progress
                    /*$("#progress-display").html(percentComplete +'%');*/

                    console.log(percentComplete);
                }
            };

            return xhr;
        },
        success: function (data) {


            var userdetails = (data["IsError"] == false) ? data["Credentials"] : "Error";
            if (userdetails != "Error") {

                $.ajax({
                    url: url + "fileupload/saveLeads/",
                    type: "post",
                    data: internal_lead,
                    cache: false,
                    success: function (data) {
                    },
                    error: function (obj, text, error) {
                        showalert(obj.responseText);
                    }

                });

            }
            /* if (userdetails == "") {
                 while (!password) {
                     var password = prompt("Please enter your password");
                     if (password != null && password != "") {
                         var cred = btoa(username + "|:|" + password);
                         window.location.replace("/thank-you/");
                         return;
                     }
                 }
             }*/
            $.ajax({
                url: url + "fileupload/sendResponseMail/",
                type: "post",
                data: { dataa: userdetails, name: 'User' },
                cache: false,
                success: function (data) {
                    //showalert(data);
                    window.location.replace("/thank-you/");
                    if ($("#firstCaptcha").val() != "No Captcha")
                        $("#firstCaptcha").val("");
                    ajax = 0;
                    $("#fileupload").trigger("reset");
                    grecaptcha.reset(widget2);
                },
                error: function (obj, text, error) {
                    showalert(obj.responseText);
                    if ($("#firstCaptcha").val() != "No Captcha")
                        $("#firstCaptcha").val("");
                    ajax = 0;
                    hideOverlay(1);
                    $("#fileupload").trigger("reset");
                    grecaptcha.reset(widget2);
                }

            });

        },
        error: function (obj, text, error) {
            showalert(obj.responseText);
            if ($("#firstCaptcha").val() != "No Captcha")
                $("#firstCaptcha").val("");
            ajax = 0;
            hideOverlay(1);
            $("#fileupload").trigger("reset");
            grecaptcha.reset(widget2);
        }

    });
});


