// =============== COUNTER WITH PLUS MINUS

function incrementValue(e) {
    e.preventDefault();
    var fieldName = $(e.target).data('field');
    var parent = $(e.target).closest('div');
    var currentVal = parseInt(parent.find('input[name=' + fieldName + ']').val(), 10);
    var actualVal = currentVal;
    if (!isNaN(currentVal)) {
        parent.find('input[name=' + fieldName + ']').val(currentVal + 1);
        actualVal = currentVal + 1;
    } else {
        parent.find('input[name=' + fieldName + ']').val(1);
        actualVal = 1;
    }
    $("#total_words").val(actualVal * 250);
}
function decrementValue(e) {
    e.preventDefault();
    var fieldName = $(e.target).data('field');
    var parent = $(e.target).closest('div');
    var currentVal = parseInt(parent.find('input[name=' + fieldName + ']').val(), 10);
    var actualVal = currentVal;
    if (!isNaN(currentVal) && currentVal > 1) {
        parent.find('input[name=' + fieldName + ']').val(currentVal - 1);
        actualVal = currentVal - 1;
    } else {
        parent.find('input[name=' + fieldName + ']').val(1);
        actualVal = 1;
    }
    $("#total_words").val(actualVal * 250);
}
$('.input-group').on('click', '.button-plus', function (e) {
    incrementValue(e);
});
$('.input-group').on('click', '.button-minus', function (e) {
    decrementValue(e);
});


/* COLLASIBLE JS*/
$('.panel-collapse').on('show.bs.collapse', function () {
    $(this).siblings('.panel-heading').addClass('active');
});

$('.panel-collapse').on('hide.bs.collapse', function () {
    $(this).siblings('.panel-heading').removeClass('active');
});


/*PRICE CALCULATOR*/

var allPagePriceCombination;
var pageValue = 0,
    dayValue = 0,
    calculatedAmount = 0;
jQuery.getJSON(site_url + "assets/public/js/Ppc.json").done(function (json) {
    allPagePriceCombination = json;
}).error(function (jqXHR, textStatus, errorThrown) { console.log(textStatus); });

var enablePagewiseDays = function (minRange, maxRange) {
    for (var i = minRange; i <= maxRange; i++) {
        var id = "#d" + i;
        jQuery(id).removeClass('disable').show();
    }
};

var calculateMinAvgBtwnPrice = function (day, avgDay, minDay, page) {
    var stndrdDayDiff = avgDay - minDay;
    var dayDiff = day - minDay;
    var avgPrice = page * 25;
    var minPrice = Math.round(page * 25 * 1.5);
    var diffPrice = Math.round(minPrice - ((minPrice - avgPrice) * dayDiff / stndrdDayDiff));
    return diffPrice;
}

var calculateAvgMaxBtwnPrice = function (day, avgDay, maxDay, page) {
    var stndrdDayDiff = maxDay - avgDay;
    var dayDiff = day - avgDay;
    var avgPrice = page * 25;
    var maxPrice = Math.round(page * 25 * 0.75);
    var diffPrice = Math.round(avgPrice - ((avgPrice - maxPrice) * dayDiff / stndrdDayDiff));
    return diffPrice;
}

var calculatePrice = function (page, day) {
    jQuery.each(allPagePriceCombination, function (index, value) {
        jQuery.each(value, function (i, itemVal) {
            if (itemVal.Page === page) {
                if (itemVal.MinDay === day)
                    calculatedAmount = Math.round(page * 25 * 1.5);

                if (itemVal.MinDay < day && itemVal.AvgDay > day)
                    calculatedAmount = calculateMinAvgBtwnPrice(day, itemVal.AvgDay, parseInt(itemVal.MinDay), page);

                if (itemVal.AvgDay === day)
                    calculatedAmount = Math.round(page * 25);

                if (itemVal.AvgDay < day && itemVal.MaxDay > day)
                    calculatedAmount = calculateAvgMaxBtwnPrice(day, itemVal.AvgDay, itemVal.MaxDay, page);

                if (itemVal.MaxDay === day)
                    calculatedAmount = Math.round(page * 25 * 0.75);
                jQuery('#calc_amount').val(calculatedAmount);
                if (calculatedAmount > 0) {
                    dayValue = calculatedAmount = 0;
                    return false;
                }
            }
        });
    });
}

jQuery('.pageCalc').on({
    click: function () {
        jQuery('.pageCalc').removeClass('active');
        jQuery(this).addClass('active');
        pageValue = 0;
        jQuery('#calc_amount').val('');
        pageValue = parseInt(jQuery(this).text());
        jQuery('.day').addClass("disable");
        jQuery('.btn-reset').addClass("disable");
        jQuery('#calc_amount').addClass("disable");
        jQuery('#aPlaceOrder').addClass("disable");

        switch (pageValue) {
            case 1:
            case 2:
            case 3:
                enablePagewiseDays(1, 4);
                break;
            case 4:
            case 5:
            case 6:
                enablePagewiseDays(1, 6);
                break;
            case 7:
            case 8:
                enablePagewiseDays(1, 7);
                break;
            case 9:
            case 10:
            case 11:
                enablePagewiseDays(2, 10);
                break;
            case 12:
                enablePagewiseDays(2, 14);
                break;
            case 13:
            case 14:
            case 15:
                enablePagewiseDays(3, 14);
                break;
        }
        if (pageValue < 4)
            jQuery("#d0-5").removeClass("disable").show();
        jQuery("#d2-5").removeClass("disable");
        jQuery('.day.disable').hide();
        jQuery('.btn-reset').removeClass("disable");
    }
});

jQuery('.day').on({
    click: function () {
        dayValue = parseFloat(jQuery(this).text());
        if (pageValue > 0 && dayValue > 0) {
            calculatePrice(pageValue, dayValue);
        }

        jQuery('#calc_amount').removeClass("disable");
        jQuery('#aPlaceOrder').removeClass("disable");
        jQuery('.cal-wrap .btm-txt').show();

    }
});

jQuery('.cal-wrap').on({
    click: function () {
        var pricelabel = window.location.href;
        /*ga('send', 'event', 'price-calculator', 'price-btn-click', pricelabel, 0, 0);*/
    }
});

jQuery('.btn-reset').on({
    click: function () {
        jQuery('.pageCalc').removeClass('active');
        pageValue = dayValue = 0;
        jQuery('.day').show().addClass("disable");
        jQuery('.btn-reset').show().addClass("disable");
        jQuery('#calc_amount').show().addClass("disable");
        jQuery('#aPlaceOrder').addClass("disable");
        jQuery('.cal-wrap .btm-txt').hide();
        jQuery('#calc_amount').val('');
        return false;
    }
});

jQuery('.view-sample').on({
    click: function () {
        var samplelabel = window.location.href;
        ga('send', 'event', 'assignemnt-view-sample', 'view-sample-click', samplelabel, 0, 0);
    }
});





