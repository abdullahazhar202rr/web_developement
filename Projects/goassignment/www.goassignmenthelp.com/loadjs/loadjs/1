var widget1;
var widget2;
var CaptchaCallback = function () {
    try {
        widget2 = grecaptcha.render('RecaptchaField2', { 'sitekey': recaptcha_sitekey, 'callback': correctCaptcha_first });

    } catch (err) {
    }
    try {
        widget1 = grecaptcha.render('RecaptchaField1', { 'sitekey': recaptcha_sitekey, 'callback': correctCaptcha_second });
    } catch (err) {
    }

};

var correctCaptcha_second = function (response) {
    $("#secondCaptcha").val(response);
};
var correctCaptcha_first = function (response) {
    $("#firstCaptcha").val(response);
};
var ip = "";
var country = "";
var countryCode = ""
var state = ""
$.getJSON('https://pro.ip-api.com/json/?key=GdLAGL6ASehXvTE', function (obj) {

    ip = obj.query;
    country = obj.country;
    countryCode = obj.countryCode;
    state = obj.region;
}).fail(function (jqXHR, textStatus, errorThrown) {
    console.log("error " + textStatus);
    console.log("incoming Text " + jqXHR.responseText);
});
