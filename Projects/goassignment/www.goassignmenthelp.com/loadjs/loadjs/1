myeventloader = function () {
    setTimeout(function () {
        window.__lc = window.__lc || {};
        window.__lc.license = 1096614;
        (function () {
            var lc = document.createElement('script');
            lc.type = 'text/javascript';
            lc.async = true;
            lc.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'cdn.livechatinctest.com/tracking.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(lc, s);
        })();
        var script_tag = document.getElementById('validation');

    }, 15000);
    /*window.dispatchEvent(new Event('customload'));*/


};
window.addEventListener("mouseup", myeventloader);
window.addEventListener("keyup", myeventloader);
window.addEventListener("scroll", myeventloader);
window.addEventListener("mouseover", myeventloader);
window.addEventListener("focus", myeventloader);
window.addEventListener("touchstart", myeventloader);
$.get("/ReadFile/readCountryCodes", function (data) {
    $("#country_codes_container").empty();
    $("#country_codes_container").append(data);
    $("#country_codes_containers").empty();
    $("#country_codes_containers").append(data);
});
});
var script_tag = document.getElementById('validation');
var page_overlay = jQuery('<div id="overlay" style="position: fixed;top: 0;left: 0;width: 100%;height:100vh !important;background-color: #000;filter:alpha(opacity=50);-moz-opacity:0.93;-khtml-opacity: 0.93;opacity: 0.93;z-index: 1000;background: url(/assets/public/images/pre-loader.gif) center no-repeat #000;background-size: 40px;"></div>');
var page_overlay1 = jQuery('<div class="progressloader"><div style="top: 50%;left: 33%;position: relative;width: 500px;"><p style="color:#000;font-weight:800;font-size:16px">Uploading...</p><progress id="progress"  value="0" class="progress"></progress></div></div>');
// Function to Add the overlay to the page
function showOverlay(version) {
    if (version === undefined)
        page_overlay.appendTo(document.body);
    else
        page_overlay1.appendTo(document.body);
}
function hideOverlay(version) {
    if (version === undefined)
        page_overlay.remove();
    else
        page_overlay1.remove();

}
var ajax = 0;
