function showCalender() {
    $("#order_date").datetimepicker("show");
}
$(document).ready(function () {
    var get_Data = function (request, response) {
        $.ajax({
            url: baseProtocol + '://' + api_url + '/api/AssignmentSubmit/GetSubjectList',
            dataType: "json",
            data: {
                term: request.term
            },
            success: function (data) {
                var parsed = JSON.parse(data);
                var newArray = new Array(parsed.length);
                var i = 0;

                parsed.forEach(function (entry) {
                    var newObject = {
                        label: entry.label,
                        value: entry.value,
                        desc: entry.desc
                    };
                    newArray[i] = newObject;
                    i++;
                });

                response(newArray);
            }
        });
    }
    var select = false;
    try {
        $("#subject").autocomplete({
            source: get_Data,
            minLength: 3, autoFocus: true, selectFirst: true,
            open: function (event, ui) {
                if (select)
                    select = false;
            },
            select: function (event, ui) {

                if (ui.item) {
                    select = true;
                    $(this).val(ui.item.label);
                    var topics = ui.item.value.split("|");
                    $("#topicId").val(topics[0]);
                    $("#subTopicId").val(topics[2]);
                    return false;
                }

            },
            focus: function (event, ui) {
                event.preventDefault();
                $(this).val(ui.item.label);
            }

        }).data("ui-autocomplete")._renderItem = function (ul, item) {
            return $("<li></li>")
                .data("item.ui-autocomplete", item)
                .append("<a><strong>" + item.label + "</strong> <br> " + item.desc + "</a>")
                .appendTo(ul);
        };
        $("#subject").blur(function () {
            if (!select)
                $(this).val("");
        });
    } catch (err) {
    }
    var arr_count = 0;

    setInterval(function () {
        var review_arr_length = all_reviews_arr.length;
        if (arr_count == review_arr_length)
            arr_count = 0;
        /*  $.notify(all_reviews_arr[arr_count], {
         allow_dismiss: false,
         animate: {
         enter: 'in-left ',
         exit: 'fadeout'
         },
         placement: {
         from: 'bottom',
         align: 'left'
         },
         delay:2000
         
         });*/
        arr_count++;
    }, 5000);
    $("#order_date").datetimepicker({ dateFormat: 'yy-mm-dd', minDate: new Date() });
    setTimeout(function () {
        $.get("/current-offers/offers/", function (data, status) {
            $("body").append(data);
        });
    }, 30000);
    $("#fileupload").validate(
        {
            rules: {
                number: { required: true, number: true }
            },
            messages: {
                name: "Enter your name",
                email: "Enter Email Id",
                number: "Enter a valid number",
                subject: "Select Subject",
                date: "Please select deadline"
            }
        });
    $("#callback").validate({
        rules: {
            phone: { required: true, number: true }
        }, messages: {
            name: "Enter your name",
            email: "Enter Email Id",
            phone: "Enter a valid number",
            subject: "Select Subject"
        }
    });
    $("#review_post").validate();
    $('.lazy').Lazy();


});
function callbackSubmit(e, currentobj) {
    e.preventDefault();
    if (ajax) {
        showalert("Already request in process");
        return
    }
    if (!$("#callback").valid()) {
        showalert("Please fill all the required fields");
        grecaptcha.reset(widget1);
        return;
    }
    if ($("#secondCaptcha").val() == "") {
        showalert("Captcha is required");
        return;
    }
    ajax = 1;
    showOverlay();
    $.ajax({
        url: url + "fileupload/callbackMail/",
        type: "post",
        data: new FormData(currentobj),
        contentType: false,
        cache: false,
        processData: false,
        success: function (data) {
            showalert(data);
            if ($("#secondCaptcha").val() != "No Captcha")
                $("#secondCaptcha").val("");
            ajax = 0;
            hideOverlay();
            $(currentobj).trigger("reset");
            grecaptcha.reset(widget1);
        },
        error: function (obj, text, error) {
            showalert(obj.responseText);
            if ($("#secondCaptcha").val() != "No Captcha")
                $("#secondCaptcha").val("");
            ajax = 0;
            hideOverlay();
            $(currentobj).trigger("reset");
            grecaptcha.reset(widget1);
        }

    });
}
function submitContactForm(obj, e) {
    e.preventDefault();
    if (ajax) {
        showalert("Already request in process");
        return
    }
    if (!$("#contactform").valid()) {
        showalert("Please fill all the required fields");
        return
    }
    if ($("#secondCaptcha").val() == "") {
        showalert("Captcha is required");
        return;
    }
    ajax = 1;
    showOverlay();
    $.ajax({
        url: url + "contact-us/",
        type: "post",
        data: new FormData(obj),
        contentType: false,
        cache: false,
        processData: false,
        success: function (data) {
            showalert(data);
            grecaptcha.reset(widget1);
            $("#secondCaptcha").val("");
            ajax = 0;
            hideOverlay();

        },
        error: function (obj, text, error) {
            showalert(obj.responseText);
            grecaptcha.reset(widget1);
            $("#secondCaptcha").val("");
            ajax = 0;
            hideOverlay();
        }

    });
}
function validateImage(imageid) {
    var fileupload = $(imageid).closest('div').find('input[type=text]');
    if ($(imageid).val() != "") {
        var file_length = $(imageid)[0].files.length;
        if (file_length > 5) {
            showalert("Total files cant be more than 5");
            $(fileupload).attr('placeholder', 'Upload File');
            return;
        }
        var total_size = 0;
        var all_name = "";
        for (var i = 0; i < file_length; i++) {
            var size = $(imageid)[0].files[i].size;
            var name = $(imageid)[0].files[i].name;
            all_name += name + " ";
            total_size += size;
        }
        if (total_size > 25000000) {
            $(imageid).val("");
            showalert("Image size cannot be more than 25MB");
            $(fileupload).attr('placeholder', 'Upload File');
        } else {
            $(fileupload).attr('placeholder', all_name);
        }

    } else
        $(fileupload).attr('placeholder', 'Upload File');

}
function showalert(msg) {
    (env == "production") ? console.log(msg) : alert(msg);
}
function postReview(obj, e) {

    e.preventDefault();
    if (ajax) {
        showalert("Already request in process");
        return
    }
    if (!$(obj).valid()) {
        showalert("Please fill all the required fields");
        return;
    }
    ;
    var star_rating = $('#star-rateit').rateit('value');
    if (star_rating <= 0) {
        showalert("Please give the ratings");
        return;
    }
    var formdata = new FormData(obj);
    formdata.append("star_rating", star_rating);
    ajax = 1;
    showOverlay();
    $.ajax({
        url: "/reviews/",
        type: "post",
        data: formdata,
        contentType: false,
        cache: false,
        processData: false,
        success: function (data) {
            showalert(data);

            ajax = 0;
            hideOverlay();
            $(obj).trigger("reset");
            $('#star-rateit').rateit('reset');
        },
        error: function (obj, text, error) {
            showalert(obj.responseText);
            ajax = 0;
            hideOverlay();
            $(obj).trigger("reset");
            $('#star-rateit').rateit('reset');
        }

    });

}
try {
    bkLib.onDomLoaded(function () {

        new nicEditor({ buttonList: [], maxHeight: 54, fullPanel: false }).panelInstance('main_form_details');
        var divcss = $(".nicEdit-panelContain").parent("div").next("div");

    });
    $('#star-rateit').rateit({
        // min value
        min: 0,
        // max value
        max: 5,
        // step size
        step: 0.5,
        // 'bg', 'font'
        mode: 'bg',
        // size of star
        starwidth: 16,
        starheight: 16,
        // is readonly?
        readonly: false,
        // is resetable?
        resetable: false,
        // is preset?
        ispreset: false,
        value: 0

    });
} catch (err) {
}
function showDropDown(event) {
    var dropdown = $(event).closest('div').next('input').next('div');
    dropdown = (typeof $(dropdown).html() == "undefined") ? $(event).closest('div').next('input').next('label').next('div') : dropdown;
    $(dropdown).show();
    var dropdown_first = $(event).attr("dropfirst");
    var dropdown_id = "#" + $(dropdown).attr("id");
    if (dropdown_first == "true") {
        var position = $(dropdown_id + " ul li a:contains('United States (+1)')").position().top;
        $(dropdown).scrollTop(position);
        $(event).attr("dropfirst", "false");
    }

}
function hideDropDown(event, footer) {
    var span = $(event).closest('div').prev('input').prev('div').find('span');
    span = (typeof $(span).html() == "undefined") ? $(event).closest('div').prev('label').prev('input').prev('div').find('span') : span;
    var span_value = $(event).attr("data-code");

    var value = (footer === undefined) ? $(event).attr("countrycode") : $(event).attr("data-code");
    $(span).html(span_value);
    (typeof $(event).closest('div').prev('input').prev('div').find('input').val() == "undefined") ? $(event).closest('div').prev('label').prev('input').prev('div').find('input').val(value) : $(event).closest('div').prev('input').prev('div').find('input').val(value);
    $(event).closest('div').hide();
}
$(document).click(function (e) {
    if (!$(e.target).hasClass("country-code")
        && $(e.target).parents("country-codes-container").length === 0) {
        $(".country-codes-container").hide();
    }
});
$(".description.questiontext").on({
    click: function () {
        if ($(".description.questiontext").text() === "Place Your Question Here") {
            $(".description.questiontext").text("").focus();
        }
    }, blur: function () {
        if ($(".description.questiontext").text() === "") {
            $(".description.questiontext").text("Place Your Question Here");
        }
    }
});

